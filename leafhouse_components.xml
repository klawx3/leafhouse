        <preDefinedFunction>INTERNET_ACCESS_LED</preDefinedFunction>
        <shutdownOptions enabled="true">
            <unexport>true</unexport>
            <pinState>LOW</pinState>
        </shutdownOptions>
        <defaultPinState>LOW</defaultPinState>
    </actuator>   
    <actuator attachedGpioPin="5">
        <name>Led rojo</name>
        <location>Cochera</location>
        <description>Sensor de puerta o ventana</description> 
        <preDefinedFunction>INTERNET_ACCESS_LED</preDefinedFunction>
