        <location>Cochera</location>
        <description>Sensor de puerta o ventana</description>          
        <pinPullResistance>PULL_DOWN</pinPullResistance>
        <onEvent></onEvent>
        <shutdownOptions enabled="true">
            <pinState>LOW</pinState>
        </shutdownOptions>
    </sensor>
    <sensor attachedGpioPin="3" type="EVENT_FIRED">
        <name>Sensor</name>
        <location>Cochera</location>
        <description>Sensor de puerta o ventana</description>          
        <pinPullResistance>PULL_DOWN</pinPullResistance>
        <onEvent>CAM</onEvent>
        <shutdownOptions enabled="true">
            <pinState>LOW</pinState>
        </shutdownOptions>
    </sensor>

    

    <actuator attachedGpioPin="4">
